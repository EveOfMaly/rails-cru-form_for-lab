<h2>Edit Song</h2>
<%= form_for(@song) do |f|  %>
  <%= f.label :name, 'Name:' %><br>
   <%= f.text_field :name %><br><br>

   <%= f.label :artist_id, 'Artists_ID:' %><br>
   <%= f.text_field :artist_id %><br><br>

   <%= f.label :genre_id, 'Genre_ID:' %><br>
   <%= f.text_field :genre_id %><br><br>
   <%= f.submit %>
<% end %>